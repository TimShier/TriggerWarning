{
"name":"TriggerWarning - Alpha",
"description":"Removes Triggering content from Twitter and Facebook.",
"version":"1",
"manifest_version":2,
"permissions": ["storage","tabs"],
"icons": { "73": "logo.jpg"},
"options_page": "options.html",
"background": {
    "scripts": ["background.js"]
  },
"content_scripts": [
    {
      "matches": ["https://twitter.com/*","http://twitter.com/*"],
      "js": ["jquery-latest.js","customScript.js"]
    }
  ]
}
