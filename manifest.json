{
"name":"TriggerWarning",
"description":"Removes Triggering content from Twitter and Facebook.",
"version":"5.2",
"manifest_version":2,
"permissions": ["storage","tabs"],
"icons": {  "16": "./icons/favicon-16.png",
            "32": "./icons/favicon-32.png",
            "48": "./icons/favicon-48.png",
            "64": "./icons/favicon-64.png",
            "114": "./icons/favicon-114.png",
            "120": "./icons/favicon-120.png",
            "144": "./icons/favicon-144.png",
            "152": "./icons/favicon-152.png",
            "256": "./icons/favicon-256.png"},
"options_page": "/options/index.html",
"background": {
    "scripts": ["background.js"]
  },
"content_scripts": [
    {
      "matches": ["https://twitter.com/*","http://twitter.com/*"],
      "js": ["jquery-latest.js","customScript.js"]
    }
  ]
}
